<%= simple_form_for(@agency) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :mail %>
    <%= f.input :address %>
    <%= f.input :postal %>
    <%= f.input :phone %>
    <%= f.input :url %>
    <%= f.input :logo %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
